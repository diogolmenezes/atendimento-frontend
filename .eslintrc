{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules":true,
      "arrowFunctions":true,
      "classes":true,
      "spread":true
    }
 },
  "extends": ["airbnb"],
  "rules": {
    "ignoreStrings": true,
    "react/forbid-prop-types": 0,
    "no-underscore-dangle": [2, { "allow": ["__INITIAL_STATE__"] }]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint"
}
